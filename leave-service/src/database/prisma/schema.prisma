generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model LeaveData {
  id         Int      @id @default(autoincrement())
  startDate  DateTime
  endDate    DateTime
  reason     String
  leaveType  String
  employeeId Int
  status     String   @default("pending")
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
